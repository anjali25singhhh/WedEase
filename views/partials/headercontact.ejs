        <div class="nav">
            <div class="text-log">
                <img src="/asset/wedease_logo.jpg" alt="wedease_logo" class="text-logo-img" height="auto" width="85px">
                <h2 class="wed-txt">WedEase</h2>
            </div>
            <div class="nav-links">
                <a href="/landing" class="nava"><h1 class="nav-txt">Home</h1></a>
                <a href="services" class="nava"><h1 class="nav-txt">Services</h1></a>
                <a href="/contact" class="nava"><h1 class="nav-txt">Contact Us</h1></a>
                <a href="/faq" class="nava"><h1 class="nav-txt">FAQ's</h1></a>
                <a href="/user-info">
                    <% if (username) { %>
                        <div class="user-info">
                            <div class="user-details">
                                <img src="/asset/profilebg.png" alt="profilebg" width="40px" height="auto">
                                <span class="username-type" style="color: #EBEAFF;"><%= username %></span>
                            </div>
                            <div class="role" style="color: #EBEAFF;">Type: <%= role %></div>
                        </div>
                    <% } %>
                </a>
                <% if (role === 'User') { %>
                    <a href="/my-bookings" class="nava"><h1 class="nav-txt">My Bookings</h1></a>
                <% } else if (role === 'Vendor') { %>
                    <a href="/vendor-my-bookings" class="nava"><h1 class="nav-txt">My Bookings</h1></a>
                <% } %>
                <a href="/chatbot" class="nava"><h1 class="nav-txt">Chat with Us</h1></a>

                
            </div>
        </div>
        
    </div>

